{
	"name": "zusam-app",
	"version": "0.1.0",
	"scripts": {
		"build": "babel src -d dist"
	},
	"babel": {
		"presets": [
			"env"
		],
		"plugins": [
			[
				"transform-react-jsx",
				{
					"pragma": "h"
				}
			]
		]
