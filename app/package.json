{
  "name": "Zusam",
  "license": "AGPL-3.0-or-later",
  "version": "0.5-a.2",
  "scripts": {
    "build": "parcel build --public-url './' src/index.html",
    "clean": "rm -rf dist/*",
    "eslint": "./node_modules/.bin/eslint --fix src",
    "lang": "cp -r src/lang dist/",
    "postbuild": "node ./build-tools/set_version_in_files.js dist/index.html dist/service-workers.js",
    "prebuild": "yarn lang",
    "prelang": "yarn clean"
  },
  "dependencies": {
    "@storeon/crosstab": "^1.0.2",
    "@storeon/localstorage": "^1.4.0",
    "bootstrap": "^5.1.3",
    "i18next": "^21.6.10",
    "idb-keyval": "^6.1.0",
    "image-blob-reduce": "^4.1.0",
    "js-cookie": "^3.0.1",
    "node-sass": "^7.0.1",
    "preact": "^10.6.4",
    "react-i18next": "^11.15.3",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "storeon": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-syntax-decorators": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@parcel/packager-raw-url": "2.2.1",
    "@parcel/transformer-image": "2.2.1",
    "@parcel/transformer-sass": "2.2.1",
    "@parcel/transformer-webmanifest": "2.2.1",
    "@size-limit/preset-app": "^7.0.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.7.0",
    "eslint-config-preact": "^1.3.0",
    "jest": "^27.4.7",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1",
    "sass": "^1.49.0",
    "size-limit": "^7.0.5"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "browserslist": "Firefox ESR, last 2 versions and > 1%, not dead, not ie <= 11",
  "size-limit": [
    {
      "path": "dist/*.js"
    }
  ],
  "packageManager": "yarn@3.0.2"
}
