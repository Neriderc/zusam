{
	"name": "zusam-app",
	"version": "0.1.0",
	"scripts": {
		"build": "parcel build src/index.html",
		"clean": "shx rm -f {public,app/dist}/*.{js,css,map,html}",
		"serve": "shx cp dist/*.{js,css,map,html} ../public",
		"full": "yarn clean && yarn build && yarn serve"
	},
	"browserslist": [
		"Chrome >= 60",
		"ChromeAndroid >= 60",
		"Edge >= 12",
		"Firefox >= 50",
		"FirefoxAndroid >= 50"
	],
	"dependencies": {
		"babel-preset-env": "^1.6.1",
		"babel-preset-hyperapp": "^2.3.0",
		"hyperapp": "^1.2.5",
		"node-sass": "^4.9.0",
		"parcel-bundler": "^1.7.0",
		"shx": "^0.2.2"
	}
}
