{
    "name": "zusam-app",
    "version": "0.1.0",
    "license": "GPL-3.0-or-later",
    "scripts": {
        "clean": "rm -f {../public,dist}/*.{js,css,map,html,png,jpg,svg}",
        "build-dev": "parcel build --no-source-maps --no-minify src/index.html",
        "build": "parcel build --no-source-maps src/index.html",
        "serve": "cp dist/* ../public; cp src/*.svg ../public",
        "preserve": "yarn build",
        "prebuild": "yarn clean"
    },
    "dependencies": {
        "babel-polyfill": "~6.26",
        "babel-preset-preact": "~1.1",
        "node-sass": "~4.11",
        "parcel-bundler": "~1.11",
        "pica": "~5.0",
        "preact": "~8"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0"
    }
}
